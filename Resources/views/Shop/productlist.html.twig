{% extends '::base.html.twig' %}
{% block title %}
    {{ productlist.title }}
{% endblock %}
{% block body %}
    <h1>{{ productlist.title }}</h1>
    {{ productlist.content }}
    {% for product in products %}
        <b><a href="{{ product.path }}">{{ product.title }}</a></b>
        <br/>
    {% endfor %}

    {{ knp_pagination_render(products) }}
{% endblock %}
